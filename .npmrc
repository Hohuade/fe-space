registry=https://registry.npmjs.org/
# registry=https://registry.npmmirror.com/
always-auth=false
save-exact=true
loglevel=warn

# Optional: Set a custom cache directory
# cache=/path/to/custom/cache

# Optional: Set a custom prefix for globally installed packages
# prefix=/path/to/custom/global

# Optional: Set a custom user configuration directory
# userconfig=/path/to/custom/userconfig

# Optional: Set a custom CA file
# cafile=/path/to/custom/cafile

# Optional: Set a custom certificate file
# cert=/path/to/custom/cert

# Optional: Set a custom key file
# key=/path/to/custom/key

# Optional: Set a custom strict-ssl setting
# strict-ssl=false